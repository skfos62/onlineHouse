<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".partner.partner_delevery_Info"
    android:padding="30sp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!--매출현황 시작-->
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="배송 조회"
            android:textColor="#000"
            android:textSize="20sp"
            android:textFontWeight="800"
            android:layout_marginTop="10dp"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp">

            <TextView
                android:id="@+id/delevery_product_Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="상품이름"
                android:textColor="#025FAF"
                android:textSize="20sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#888"
            android:layout_marginTop="15sp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="운송장번호"
                android:textColor="@color/btn_default"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:layout_weight="1"
                />

            <TextView
                android:id="@+id/delevery_product_Num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="운송장번호"
                android:textColor="#000"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:gravity="right"
                android:layout_weight="1"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="택배회사"
                android:textColor="@color/btn_default"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:layout_weight="1"
                />

            <TextView
                android:id="@+id/delevery_product_companyName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="택배회사이름"
                android:textColor="#000"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:gravity="right"
                android:layout_weight="1"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주소"
                android:textColor="@color/btn_default"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:layout_weight="1"
                />

            <TextView
                android:id="@+id/delevery_product_adress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주소이름"
                android:textColor="#000"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:gravity="right"
                android:layout_weight="1"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:background="@color/btn_default">

            <TextView
                android:id="@+id/partner_delevery_Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="시간"
                android:gravity="center"
                android:textColor="#000"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:textStyle="bold"
                android:layout_weight="1"
                />

            <TextView
                android:id="@+id/partner_delevery_Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="현재위치"
                android:textColor="#000"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:textStyle="bold"
                android:gravity="center"
                android:layout_weight="1"
                />
            <TextView
                android:id="@+id/partner_delevery_Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="배송상태"
                android:textColor="#000"
                android:textSize="15sp"
                android:textFontWeight="800"
                android:textStyle="bold"
                android:gravity="center"
                android:layout_weight="1"
                />
        </LinearLayout>

        <TextView
            android:id="@+id/partner_delevery_wait_recycler"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_marginTop="10sp"
            android:layout_marginBottom="10sp"
            android:text="해당 운송장은 처리중입니다."
            android:layout_marginLeft="80sp"
            >
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <!--    <TextView-->
            <!--        android:layout_width="wrap_content"-->
            <!--        android:layout_height="wrap_content"-->
            <!--        android:text="2020.05.06"-->
            <!--        android:textColor="#e5e5e5"-->
            <!--        android:textSize="20sp"-->
            <!--        android:textFontWeight="800"-->

            />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="bottom"
            android:visibility="invisible">
            <TextView
                android:id="@+id/partner_delevery_Home_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="홈"
                android:textColor="#000"
                android:textSize="20sp"
                android:textFontWeight="800"
                android:layout_marginTop="10sp"
                android:layout_weight="1"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/partner_delevery_product_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="상품 등록"
                android:textColor="#000"
                android:textSize="20sp"
                android:textFontWeight="800"
                android:layout_marginTop="10sp"
                android:layout_weight="1"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/partner_delevery_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="배송관리"
                android:textColor="#000"
                android:textSize="20sp"
                android:textFontWeight="800"
                android:layout_marginTop="10sp"
                android:layout_weight="1"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/partner_main_chatt_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="고객상담방"
                android:textColor="#000"
                android:textSize="20sp"
                android:textFontWeight="800"
                android:layout_marginTop="10sp"
                android:layout_weight="1"
                android:gravity="center"
                />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>